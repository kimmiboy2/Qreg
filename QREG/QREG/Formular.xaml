<?xml version="1.0" encoding="utf-8" ?>
<QREG:CoolContentPage 
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:QREG="clr-namespace:QREG;
    assembly=QREG"
    x:Class="QREG.Formular"             
    EnableBackButtonOverride="true"
    BackgroundColor="LightBlue">

    <StackLayout x:Name="FormularRootView">
        <ScrollView VerticalOptions="StartAndExpand">
            <StackLayout Orientation="Vertical" x:Name="FormularLayout">
            </StackLayout>
        </ScrollView>
        <StackLayout VerticalOptions="End" >
            <Button Text="Indsend" Clicked="Button_Clicked"></Button>
        </StackLayout>
    </StackLayout>
</QREG:CoolContentPage>